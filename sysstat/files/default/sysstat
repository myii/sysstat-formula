{%- macro render_key_value_pairs(cfg) %}
{%-   for k,v in cfg.items() -%}
{{ k }} = {{ v }}
{%    endfor %}
{%- endmacro %}

{{ render_key_value_pairs(config.get("default", {})) }}
{% for section, cfg in config.items() %}
{%-   if section == "default" %}{% continue %}{% endif %}
[{{section}}]
{{ render_key_value_pairs(cfg) }}
{%  endfor %}
